{
   "silent" : false,
   "paraview": {
      "each-timestep": true,
      "directory": "vortex-steady"
   },
   "flow-param": {
      "entropy-state": false,
      "mach": 1.0,
      "aoa": 0.0
   },
   "space-dis": {
      "degree": 1,
      "lps-coeff": 1.0,
      "basis-type": "csbp",
      "flux-fun": "IR"
   },
   "time-dis": {
      "type": "PTC",
      "steady": true,
      "steady-abstol": 1e-12,
      "steady-restol": 1e-10,
      "t-final": 100,
      "dt": 1e12,
      "cfl": 1.0,
      "res-exp": 2.0
   },
   "bcs": {
      "vortex": [1, 1, 1, 0],
      "slip-wall": [0, 0, 0, 1]
   },
   "nonlin-solver": {
      "printlevel": 1,
      "maxiter": 50,
      "reltol": 1e-1,
      "abstol": 1e-12
   },
   "lin-solver": {
      "type": "hyprefgmres",
      "printlevel": 0,
      "filllevel": 3,
      "maxiter": 100,
      "reltol": 1e-2,
      "abstol": 1e-12
   },
   "saveresults":false,
   "outputs":
   {
      "each-timestep": true,
      "entropy": {},
      "drag": {
         "boundaries": [0, 0, 0, 1]
      }
   }
}
