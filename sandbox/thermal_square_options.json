{
    "mesh": {
       "file": "initial.mesh",
       "num-edge-x": 2,
       "num-edge-y": 2,
       "num-edge-z": 2
    },
    "space-dis": {
       "basis-type": "H1",
       "degree": 1,
       "GD": false
    },
    "steady": false,
    "time-dis": {
        "ode-solver": "MIDPOINT",
        "const-cfl": true,
        "cfl": 1.0,
        "dt": 0.01,
        "t-final": 0.02
    },
    "lin-prec": {
        "type": "hypreboomeramg"
    },
    "lin-solver": {
        "reltol": 1e-14,
        "abstol": 0.0,
        "printlevel": 0,
        "maxiter": 500
     },
    "adj-solver":{
       "reltol": 1e-8,
       "abstol": 0.0,
       "printlevel": 0,
       "maxiter": 500
    },
    "motor-opts" : {
       "current": 1,
       "frequency": 1500
    },
    "components": {
       "stator": {
          "material": "testmat",
          "attr": 1,
          "max-temp": 0.5
       },
       "rotor": {
          "material": "testmat",
          "attr": 2,
          "max-temp": 0.5
       }
    },
    "bcs": {
        "outflux": [0, 0, 0, 1, 1, 0],
        "const": true,
        "const-val": 1
    },
    "outflux-type": "test",
    "outputs": {
        "temp-agg": "temp-agg"
    },
    "rho-agg": 10,
    "max-temp": 0.1,
    "init-temp": 1,
    "compute-sens": true,
    "verify-full": false,
    "verify-cd": true,
    "mesh-move-opts-path": "thermal_mesh_move_options.json",
    "material-lib-path": "../../src/material_options.json"
 }