{
   "mesh": {
      "file": "wire_em.smb",
      "out-file": "wire_em_out",
      "model-file": "wire_em.egads"
   },
   "space-dis": {
      "basis-type": "nedelec",
      "degree": 2
   },
   "steady": true,
   "lin-solver": {
      "type": "hypregmres",
      "pctype": "hypreams",
      "rel-tol": 1e-10,
      "abs-tol": 0.0,
      "printlevel": 2,
      "maxiter": 50
   },
   "nonlin-solver": {
      "printlevel": 1,
      "reltol": 1e-9
   },
   "components": {
      "wire": {
         "material": "copperwire",
         "linear": true,
         "attr": 1
      },
      "farfields": {
         "material": "air",
         "linear": true,
         "attr": 2
      }
   },
   "problem-opts": {
      "fill-factor": 1.0,
      "current-density": 1.0,
      "current": {
         "z": [1]
      }
   },
   "outputs": [
      "energy",
      "co-energy"
   ]
}
