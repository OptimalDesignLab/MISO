{  
   "silent" : false,
   "paraview": {
      "each-timestep": true,
      "directory": "forced-box"
   },
   "flow-param": {
      "entropy-state": false,
      "mach": 0.2
   },
   "space-dis": {
      "degree": 1,
      "lps-coeff": 1.0,
      "basis-type": "csbp",
      "flux-fun": "IR"
   },
   "time-dis": {
      "type": "RRK6",
      "exact-t-final": false,
      "steady": false,
      "t-final": 5,
      "dt": 0.01,
      "cfl": 1.0,
      "entropy-log": true
   },
   "nonlin-solver": {
      "printlevel": 1,
      "maxiter": 50,
      "reltol": 1e-6,
      "abstol": 1e-10
   },
   "lin-solver": {
      "type": "fgmres",
      "printlevel": 1,
      "filllevel": 3,
      "maxiter": 5,
      "kdim": 5,
      "reltol": 1e-2,
      "abstol": 1e-12
   },
   "lin-prec": {
      "type": "hypreilu",
      "lev-fill": 0
   },
   "bcs": {
      "slip-wall": [1, 1, 1, 0],
      "control": [0, 0, 0, 1]
   },
   "saveresults":false,
   "outputs":
   { 
      "boundary-entropy": {
         "boundaries": [0, 0, 0, 1]
      },
      "entropy": {}
   }
}