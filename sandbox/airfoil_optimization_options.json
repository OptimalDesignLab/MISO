{
   "problem-type": "airfoil",
   "flow-param": {
      "mach": 0.8,
      "aoa": 0.0,
      "roll-axis": 0,
      "pitch-axis": 1
   },
   "space-dis": {
      "basis-file":"airfoil_p2_r0.mesh",
      "DGD": true,
      "DGD-degree": 1,
      "extra-basis": 4,
      "condv": 10.0,
      "degree": 1,
      "lps-coeff": 1.0,
      "basis-type": "dsbp",
      "print-level": 1
   },
   "BFGSNewton":{
      "alpha-init": 0.1,
      "alpha-max": 1e4,
      "c1":1e-4,
      "c2":0.9,
      "max-iter":20,
      "zoom-max-iter":60,
      "print-level": 1,
      "abs-tol":1e-12,
      "rel-tol":1e-6,
      "update":true,
      "updatecircle":5
   },
   "steady": false,
   "time-dis": {
      "const-cfl": true,
      "res-exp": 2.0,
      "ode-solver": "MIDPOINT",
      "t-final": 100,
      "dt": 0.1,
      "cfl": 1.0,
      "max-iter": 1e4
   },
   "mesh": {
      "file": "naca0012-r1.mesh",
      "refine": 0
   },
   "bcs": {
      "slip-wall": [0, 0, 1, 1],
      "far-field": [1, 1, 0, 0]
   },
   "newton": {
      "printlevel": 1,
      "maxiter": 50,
      "reltol": 1e-10,
      "abstol": 1e-12
   },
   "petscsolver": {
      "ksptype": "gmres",
      "pctype": "lu",
      "abstol": 1e-15,
      "reltol": 1e-15,
      "maxiter": 100,
      "printlevel": 0
   },
   "lin-solver": {
      "printlevel": 0,
      "filllevel": 3,
      "maxiter": 100,
      "reltol": 1e-2,
      "abstol": 1e-12
   },
   "saveresults":false,
   "outputs":
   {
      "drag": [0, 0, 1, 1]
   },
   "shock-capturing": {
      "use": false,
      "sensor-param": 0
   } 
}
