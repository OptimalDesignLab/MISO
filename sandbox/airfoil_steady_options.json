{
   "flow-param": {
      "mach": 0.5,
      "aoa": 3.0,
      "roll-axis": 0,
      "pitch-axis": 1
   },
   "space-dis": {
      "RBF": false,
      "RBF-degree": 1,
      "extra-basis": 4,
      "shape-param": 10.0,
      "degree": 1,
      "lps-coeff": 1.0,
      "basis-type": "csbp"
   },
   "steady": false,
   "time-dis": {
      "const-cfl": true,
      "ode-solver": "MIDPOINT",
      "t-final": 100,
      "dt": 0.1,
      "cfl": 1.0
   },
   "mesh": {
	   "file": "airfoil_steady.mesh",
      "refine": 0
   },
   "bcs": {
      "slip-wall": [0, 0, 1, 1],
      "far-field": [1, 1, 0, 0]
   },
   "newton": {
      "printlevel": 1,
      "maxiter": 50,
      "reltol": 1e-14,
      "abstol": 1e-14
   },
   "petscsolver": {
      "ksptype": "gmres",
      "pctype": "lu",
      "abstol": 1e-15,
      "reltol": 1e-15,
      "maxiter": 100,
      "printlevel": 0
   },
   "lin-solver": {
      "printlevel": 0,
      "filllevel": 3,
      "maxiter": 100,
      "reltol": 1e-2,
      "abstol": 1e-12
   },
   "saveresults":false,
   "outputs":
   {
      "drag": [0, 0, 1, 1]
   }
}
