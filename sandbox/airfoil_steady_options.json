{
   "flow-param": {
      "mach": 0.5,
      "aoa": 3.0,
      "roll-axis": 0,
      "pitch-axis": 1,
      "Re": 100.0,
      "Pr": 0.75
   },
   "space-dis": {
      "GD": false,
      "GD-degree": 1,
      "degree": 1,
      "lps-coeff": 1.0,
      "basis-type": "csbp"
   },
   "time-dis": {
      "steady": true,
      "steady-abstol": 1e-12,
      "steady-reltol": 1e-10,
      "res-exp": 2.0,
      "const-cfl": true,
      "ode-solver": "PTC",
      "t-final": 100,
      "dt": 0.1,
      "cfl": 1.0
   },
   "mesh": {
	   "file": "airfoil_steady.mesh",
      "refine": 0
   },
   "bcs": {
      "no-slip-adiabatic": [0, 0, 1, 1],
      "far-field": [1, 1, 0, 0]
   },
   "nonlin-solver": {
      "printlevel": 1,
      "maxiter": 50,
      "reltol": 1e-1,
      "abstol": 1e-12
   },
   "petscsolver": {
      "ksptype": "gmres",
      "pctype": "lu",
      "abstol": 1e-15,
      "reltol": 1e-15,
      "maxiter": 100,
      "printlevel": 0
   },
   "lin-solver": {
      "type": "hyprefgmres",
      "printlevel": 0,
      "maxiter": 100,
      "reltol": 1e-2,
      "abstol": 1e-12
   },
   "lin-prec": {
      "type": "hypreilu",
      "lev-fill": 4
   },
   "adj-prec": {
      "type": "hypreilu",
      "lev-fill": 4
   },
   "saveresults":false,
   "outputs":
   {
      "drag": [0, 0, 1, 1]
   }
}
