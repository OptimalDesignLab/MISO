{
   "problem-type": "a",
   "space-dis": {
      "DGD": true,
      "DGD-degree": 1,
      "extra-basis": 4,
      "condv": 10.0,
      "degree": 1,
      "lps-coeff": 1.0,
      "basis-type": "dsbp"
   },
   "BFGSNewton":{
      "alpha-init": 1e-1,
      "alpha-max": 1e3,
      "c1":1e-4,
      "c2":0.9,
      "max-iter":200,
      "zoom-max-iter":50,
      "print-level": 1,
      "abs-tol":1e-12,
      "rel-tol":1e-6,
      "update":true,
      "updatecircle":10
   },
   "steady": true,
   "time-dis": {
      "const-cfl": true,
      "ode-solver": "MIDPOINT",
      "t-final": 100,
      "dt": 0.1,
      "cfl": 1.0,
      "res-exp":1.0
   },
   "mass-matrix":
   {
      "lump": false
   },
	"model-file": "annulus.dmg",
   "mesh": {
	   "file": "annulus.smb",
      "refine": 0
   },
   "bcs": {
      "vortex": [1, 1, 1, 0],
      "slip-wall": [0, 0, 0, 1]
   },
   "newton": {
      "printlevel": 1,
      "maxiter": 50,
      "reltol": 1e-14,
      "abstol": 1e-14
   },
   "petscsolver": {
      "ksptype": "gmres",
      "pctype": "lu",
      "abstol": 1e-15,
      "reltol": 1e-15,
      "maxiter": 100,
      "printlevel": 2
   },
   "lin-solver": {
      "printlevel": 0,
      "filllevel": 3,
      "maxiter": 100,
      "reltol": 1e-2,
      "abstol": 1e-12
   },
   "saveresults":false,
   "outputs":
   {
      "entropy": true,
      "drag": [0, 0, 0, 1]
   },
   "shock-capturing":
   {
      "use": false
   }
}
